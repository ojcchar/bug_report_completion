      break;
      case 3 : echo "Partners".</sentence>
            <sentence id="9.5">$hr; 
      break;
      
   }
   
?</sentence>
            <sentence id="9.6">&gt;
&lt;/BODY&gt;
&lt;/HTML&gt;
[/file]</sentence>
        </paragraph>
    </description>
</bug>
